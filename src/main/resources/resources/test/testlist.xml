<?xml version="1.0" encoding="UTF-8"?>
<testlist>
	<test name="Insert Data into MongoDB">
		<request>
			<identifier>active:mongoDB</identifier>
			<argument method="as-string" name="dbserver">localhost:27017</argument>
			<argument method="as-string" name="dbname">test</argument>
			<argument method="as-string" name="collection">test</argument>
			<argument method="as-string" name="action">insert</argument>
			<argument method="as-string" name="data">{"hello":"hallo", "shalbs":"shalbs2"}</argument>
		</request>
		<assert>
			<notNull/>
		</assert>
	</test>
</testlist>